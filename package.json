    "grunt-contrib-watch": "^1.1.0",
    "grunt-eslint": "^23.0.0",
    "grunt-msbuild": "^1.1.1",
    "grunt-prettier": "^2.1.0",
    "grunt-prompt": "^1.3.3",
    "grunt-shell": "^3.0.1",
    "load-grunt-tasks": "^5.1.0",
    "local-repl": "^4.0.0",
    "minimist": "^1.2.5",
